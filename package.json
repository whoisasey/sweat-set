{
  "name": "workout-app",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint app/**/*.tsx && stylelint app/*.scss",
    "lint:fix": "eslint app/**/*.tsx && stylelint app/*.scss --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: tests not yet implemented\" && exit 0",
    "test:coverage": "echo \"Error: tests not yet implemented\" && exit 0",
    "prepare": "husky",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:staged": "prettier --write $(git diff --cached --name-only --diff-filter=d | sed 's|^www/||' | grep -E '\\.(ts|tsx|js|jsx|json)$' | xargs)",
    "backfill:supabase": "npx tsx scripts/backfill-users-to-supabase.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/x-date-pickers": "^8.11.3",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/supabase-js": "^2.86.2",
    "bcryptjs": "^3.0.2",
    "d3-shape": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "intro.js": "^8.3.2",
    "jose": "^6.1.3",
    "js-cookie": "^3.0.5",
    "mongodb": "^6.12.0",
    "mongoose": "^9.0.2",
    "next": "^16.1.0",
    "next-auth": "^4.24.12",
    "prettier": "^3.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.6.0",
    "sass": "^1.97.0",
    "swiper": "^12.0.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "stylelint": "^16.6.1",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.3.1",
    "typescript": "^5",
    "typescript-eslint": "^8.50.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.css": [
      "stylelint"
    ]
  }
}
